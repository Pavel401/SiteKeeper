{
  "name": "SiteKeeper",
  "version": "0.1.0",
  "description": "SiteKeeper is a simple bookmark management extension designed to safeguard and organize your favorite websites. With SiteKeeper, you can effortlessly bookmark, store, and categorize web pages, ensuring easy access and efficient organization.",
  "icons": {
    "16": "assets/logo.png",
    "24": "assets/logo.png",
    "32": "assets/logo.png"
  },
  "permissions": ["storage", "tabs"],
  "host_permissions": [],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://*.youtube.com/*"],
      "js": ["contentScript.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "assets/logo.png"
     
      ],
      "matches": ["https://*.youtube.com/*"]
    }
  ],
  "action": {
    "default_icon": {
      "16": "assets/logo.png",
      "24": "assets/logo.png",
      "32": "assets/logo.png"
    },
    "default_title": "SiteKeeper",
    "default_popup": "popup.html"
  },
  "manifest_version": 3
}
